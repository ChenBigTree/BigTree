<view class="home">
	<view class="title">
		<!-- <w-tabs bind:onChange="handleChange" options="{{ tabs1 }}" /> -->
		<!-- <v-tabs /> -->
		<i-tabs current="{{ current_scroll }}" scroll bindchange="handleChangeScroll">
			<i-tab class="i-tab" wx:for="{{tabs1}}" wx:key="index" key="{{index+1}}" data-key="{{index+1}}" title="{{item}}"></i-tab>
			<i-tab class="i-tab" wx:for="{{lists}}" wx:key="index" key="{{index+5}}" data-name="{{item.tag}}" title="{{item.tag}}"></i-tab>
		</i-tabs>
		<view class="title-box">
			<view class="image-box" data-name="listPage" bindtap="showListPage">
				<image mode="widthFix" src="../../images/icon/san.png"></image>
			</view>
		</view>
	</view>

	<view class="listPage-box {{class}}" data-name="listPage-box" bindtap="showListPage">
		<view class="listPage clearfix" data-name="listPage" catchtap="showListPage">
			<view class="list-box" wx:for="{{tabs1}}" wx:key="index" key="{{index+1}}" data-key="{{index+1}}" catchtap="showListPage" data-name="{{item}}" title="{{item}}">{{item}}
			</view>
			<view class="list-box" wx:for="{{lists}}" wx:key="index" key="{{index+5}}" data-key="{{index+5}}" catchtap="showListPage" data-name="{{item.tag}}" title="{{item.tag}}">{{item.tag}}</view>
		</view>
	</view>


	<view class="top-circle">
		<block wx:if="{{wallData != ''}}">
  
		</block>

		<view class="null" wx:else>暂无内容</view>
	</view>
</view>