/* components/circlePage/index.wxss */
.top-circle {
  margin-top: 22rpx;
}

.circle {
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  border-bottom: 2rpx solid #ececec;
  font-family: Arial, sans-serif;
  margin-right: 20rpx;
}

.userhead {
  width: 72rpx;
  height: 72rpx;
  border-radius: 8rpx;
  margin-left: 16rpx;
}

.comments {
  background: #F7F7F7;
  font-size: 14px;
  padding: 5px;
}

.row {
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
}

.col {
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
  width: 100%;
}

.username {
  width: calc(100% - 100rpx);
  color: #576b95;
  font-size: 32rpx;
  font-family: Arial, sans-serif;
  overflow: hidden;
  font-weight: 600;
  float: left;
}

.isPutaway {
  width: 100rpx;
  float: right;
  text-align: center;
  background: #c8c087;
  border-radius: 5px;
  padding: 1px 0;
  font-size: 28rpx;
}

.oneTextOverflow {
  text-align: justify;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.content {
  font-size: 32rpx;
  font-family: Arial, sans-serif;
  overflow: hidden;
  color: #252525;
  margin-bottom: 10rpx;
}

.threeTextOverflow {
  text-align: justify;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

.tweTextOverflow {
  text-align: justify;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.community-title-box {
  width: 400rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.community-li-title-none {
  position: absolute;
  width: 380rpx;
  color: #c8c087;
  top: 40rpx;
  left: 45rpx;
  font-weight: bolder;
  height: 88rpx;
}

.community-li-title-avatar-box {
  position: relative;
  bottom: 100rpx;
}

.community-li-title-avatar {
  width: 70rpx;
  height: 70rpx;
  overflow: hidden;
  border-radius: 50rpx;
  position: absolute;
  right: 220rpx;
  bottom: 160rpx;
}

.community-li-title-name {
  width: 50rpx;
  position: absolute;
  right: 230rpx;
  font-size: 25rpx;
  color: #333;
  bottom: 120rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis
}

.userIcon {
  width: 70rpx;
  height: 70rpx;
}

.li-imgs {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 8rpx;
}

.community-li-title-time {
  position: absolute;
  top: 200rpx;
  left: 100rpx;
  font-size: 25rpx;
}

.community-title-box {
  width: 300rpx;
  height: 100rpx;
  margin-left: 0rpx;
}

.communityPage-li-img-none {
  position: relative;
}

.community-li-title-avatar image {
  width: 100%;
}

.uploadImg {
  border-radius: 10rpx;
  position: absolute;
  top: 115rpx;
  left: 255rpx;
}

.timeArea {
  font-size: 8pt;
  line-height: 20pt;
  color: #888;
  margin-top: 6rpx;
  position: relative;
}

.timeArea-right {
  vertical-align: bottom;
}

.dd-icon {
  height: 32rpx;
  width: 32rpx;
}

comm-zan {
  width: 100%;
}

.r {
  border-bottom: 16rpx solid #f7f7f7;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  width: 2rpx;
  margin-top: 5rpx;
  margin-left: 20rpx;
}

.zan {
  overflow: hidden;
  background-color: #f7f7f7;
  align-items: center;
  border-bottom: 1rpx solid #ececec;
  padding: 12rpx 6rpx 8rpx 6rpx;
}

.love-icon {
  float: right;
  padding-left: 8rpx;
  padding-right: 2rpx;
  width: 32rpx;
  height: 32rpx;
  padding-top: 4rpx;
}

.like {
  font-size: 32rpx;
  line-height: 44rpx;
  color: #576b95;
}

.null {
  font-size: 14px;
  text-align: center;
  padding: 30px 0;
  color: #6b6b6b;
}

.wuss-tabs-content {
  width: 100%;
}

.wuss-tabs-content>view,
.wuss-tab-line-bar {
  width: calc(100% / 5) !important;
}

.title {
  padding: 0 40px 0 0;
  height: 42px;
  position: relative;
}

.title-box {
  width: 40px;
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
}

.image-box {
  width: 30px;
  height: 30px;
  margin: 5px;
}

.image-box>image {
  width: 100%;
  height: 100%;
}

.i-tab view {
  width: 85px;
  display: inline-block;
}

.null {
  text-align: center;
  font-size: 14px;
  color: #6b6b6b;
  padding: 20px 0;
}

.listPage-box {
  width: 100%;
  height: 100%;
  position: fixed;
  top: -100%;
  left: 0px;
  z-index: 9999;
  transition: top 0.5s linear;
}

.listPage {
  width: 100%;
  background: #fff;
  padding: 10px;
  box-sizing: border-box;
  box-shadow: 0 5px 5px #BFBFBF;
}

.top {
  top: 0;
}

.bottom {
  top: -100%;
}

.list-box {
  width: calc((100% - 25px) / 3);
  height: 60rpx;
  line-height: 60rpx;
  float: left;
  border-radius: 5px;
  text-align: center;
  margin: 0 8px 5px 0;
  border: 1px solid #BFBFBF;
  box-shadow: 0 0 3px #999;
}

.list-box:nth-child(3n) {
  margin: 0 0 5px 0;
}