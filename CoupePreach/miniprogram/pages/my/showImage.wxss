.imagePage {
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  width: 100%;
  height: 100%;
  z-index: 666;
  display: flex;
  align-items: center;
}

.image-text {
  padding: 6rpx 14rpx;
  border: 1px solid #f2f2f2;
  position: fixed;
  top: 30rpx;
  right: 30rpx;
  color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
}

.image-box {
  width: 100%;
}

.image-box>image {
  width: 100%;
}
