page {
  width: 100%;
  height: 100%;
}

.clearfix::after {
  content: '';
  display: block;
  clear: both;
}

.image {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.fl {
  float: left;
}

.fr {
  float: right;
}


.towTextOverflow {
  text-align: justify;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}