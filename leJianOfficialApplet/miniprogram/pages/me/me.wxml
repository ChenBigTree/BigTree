<view class="me">
	<view class="me-message-box">
		<view class="me-message" wx:if="{{userInfo}}">
			<view class="avatar">
				<open-data class="avatar" type="userAvatarUrl"></open-data>
			</view>
			<view class="me-name">
				<open-data type="userNickName"></open-data>
			</view>
		</view>
		<view class="me-message" bindtap="openConfirm" wx:if="{{!userInfo}}">
			<view class="me-imgbox">
				<image mode="withFix" src="../icon/nologin.jpg"></image>
			</view>
			<view>请登录</view>
		</view>
	</view>
	<view class="me-list">
		<view class="list" data-url="../me/business/business" bindtap="openConfirm">
			<view>商家后台</view>
		</view>
		<view class="list" data-url="../me/myFavorite/myFavorite" bindtap="openConfirm">
			<view>我的收藏</view>
		</view>
		<view class="list" data-url="../me/administratorEntry/administratorEntry" bindtap="openConfirm" wx:if="{{isAdministrator&&userInfo}}">
			<view>管理员入口</view>
		</view>
		<view class="list" data-url="../me/feedback/feedback" bindtap="openConfirm">
			<view>反馈意见</view>
		</view>
		<view class="list" data-url="../me/service/service" bindtap="openConfirm">
			<view>联系客服</view>
		</view>
	</view>

	<mp-auth show="{{dialogShow}}" bindgetUserInfo="getUserInfo"></mp-auth>

</view>