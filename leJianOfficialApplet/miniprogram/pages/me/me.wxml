<view class="me">
	<view class="me-message-box">
		<view class="me-message" wx:if="{{userInfo}}">
			<view class="avatar">
				<open-data class="avatar" type="userAvatarUrl"></open-data>
			</view>
			<view class="me-name">
				<open-data type="userNickName"></open-data>
			</view>
		</view>
		<view class="me-message" bindtap="openConfirm" wx:if="{{!userInfo}}">
			<view class="me-imgbox">
				<image mode="withFix" src="../icon/nologin.jpg"></image>
			</view>
			<view>请登录</view>
		</view>
	</view>
	<view class="me-list">
		<view class="list">
			<navigator hover-class="none" url="../me/business/business">商家后台</navigator>
		</view>
		<view class="list">
			<navigator hover-class="none" url="../me/myFavorite/myFavorite">我的收藏</navigator>
		</view>
		<view class="list" hidden="{{isAdministrator}}">
			<navigator hover-class="none" url="../me/administratorEntry/administratorEntry">管理员入口</navigator>
		</view>
		<view class="list">
			<navigator hover-class="none" url="../me/investmentPromoter/investmentPromoter">成为招商员</navigator>
		</view>
		<view class="list">
			<navigator hover-class="none" url="../me/feedback/feedback">反馈意见</navigator>
		</view>
		<view class="list">
			<navigator hover-class="none" url="../me/service/service">联系客服</navigator>
		</view>
	</view>

	<mp-auth show="{{dialogShow}}" bindgetUserInfo="getUserInfo"></mp-auth>

</view>